    "scaffolder",
    "project-setup",
    "devtools"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "ajv": "^8.12.0",
    "jsonc-parser": "^3.2.0",
    "yaml": "^2.3.4"
  }
}
