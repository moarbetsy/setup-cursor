{
  "name": "precursor",
  "version": "1.0.0",
  "description": "Config-driven, idempotent project doctor + scaffolder for Cursor",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun",
